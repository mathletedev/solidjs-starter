{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"module": "esnext",
		"moduleResolution": "node",
		"noEmit": true,
		"strict": true,
		"target": "esnext",
		"types": ["vite/client"]
	}
}
